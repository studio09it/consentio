___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const setDefaultConsentState = require('setDefaultConsentState');
const injectScript = require('injectScript');
const setInWindow = require('setInWindow');
const gtagSet = require('gtagSet');
const makeNumber = require('makeNumber');

// Configurazione
const scriptKey = data.scriptKey;
const apiBase = 'https://consentio.it';
const lang = data.lang === 'auto' ? null : data.lang;
const waitForUpdate = makeNumber(data.waitForUpdate) || 500;

// Imposta i default del consenso (GDPR compliant: tutto denied di default)
const defaultConsent = {
  'analytics_storage': data.analytics_storage_default ? 'granted' : 'denied',
  'ad_storage': data.ad_storage_default ? 'granted' : 'denied',
  'ad_user_data': data.ad_user_data_default ? 'granted' : 'denied',
  'ad_personalization': data.ad_personalization_default ? 'granted' : 'denied',
  'functionality_storage': 'denied',
  'personalization_storage': 'denied',
  'security_storage': 'granted',
  'wait_for_update': waitForUpdate
};

log('Consentio: Setting default consent state', defaultConsent);
setDefaultConsentState(defaultConsent);

// Imposta opzioni gtag avanzate
if (data.urlPassthrough) {
  gtagSet('url_passthrough', true);
}
if (data.adsDataRedaction) {
  gtagSet('ads_data_redaction', true);
}

// Configura Consentio
const consentioConfig = {
  scriptKey: scriptKey,
  apiBase: apiBase
};

if (lang) {
  consentioConfig.lang = lang;
}

setInWindow('ConsentioConfig', consentioConfig, true);

// Carica lo script di Consentio
const scriptUrl = apiBase + '/widget/consentio.js';

injectScript(scriptUrl, function() {
  log('Consentio: Widget loaded successfully');
  data.gtmOnSuccess();
}, function() {
  log('Consentio: Failed to load widget');
  data.gtmOnFailure();
}, 'consentio');


